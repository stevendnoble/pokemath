// Prevents scrolling on the page
body, 
html {
	height: 100%;
}

body {
	overflow: hidden;
}

div.wrapper {
  height: 100%;
  overflow: hidden;
}

body {
  margin-top: 70px;
}

// styling for game tiles
.game-tile {
	background-color: green;
	padding: 0;
  background-image: url('grass.png');
}

.obstacle0-tile {
  content: url('tree.png');  
}

.obstacle1-tile {
  content: url('stump.png');  
}

.obstacle2-tile {
  content: url('log.png');  
}