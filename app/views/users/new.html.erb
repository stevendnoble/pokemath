<h1>Sign Up</h1>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for :user, url: '/users' do |f| %>
      Username: <%= f.text_field :username %>
      Password: <%= f.text_field :password %>
      Password Confirmation: <%= f.password_field :password_confirmation %>
      <%= f.submit "Submit" %>
      <% end %>
    </div>
  </div>
</div>